<div class="container">

  <h1><%=current_user.id%> <%=@survey.title%></h1>
    <%@survey.questions.each do |question|%>
    <p><%=question.content%></p>
    <ul>
      <%question.choices.each do |choice|%>
        <li><%=choice.content%> || <%=choice.responses.count%></li>
      <% end %>
    </ul>
    <% end %>

</div>
